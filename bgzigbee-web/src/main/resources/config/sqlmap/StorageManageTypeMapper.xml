<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.bgzigbee.manage.dao.StorageManageTypeMapper">

	<select id="findAll" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.StorageManageTypeEntity">
		SELECT *
		FROM `storagemanagetype` order by `id` ASC
	</select>
    <insert id="addStorageManageType" parameterType="com.smartcold.bgzigbee.manage.entity.StorageManageTypeEntity"
    useGeneratedKeys="true" keyProperty="id">
        INSERT INTO `storagemanagetype`(
		`type`,
		`addTime`
		)
		VALUE 
		(
		#{type},
		now()
		)
     </insert>
     <delete id="deleteStorageManageType" parameterType="map">
	    DELETE FROM storagemanagetype WHERE `id` = #{id}
	</delete>
</mapper>