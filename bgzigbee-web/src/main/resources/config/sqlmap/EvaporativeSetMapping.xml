<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smartcold.bgzigbee.manage.dao.EvaporativeSetMapping">
    <insert id="insert">
        INSERT INTO `evaporativeset` (`rdcid`, `name`)
        VALUES (#{rdcid}, #{name})
    </insert>
    <select id="findById" parameterType="map" resultType="EvaporativeSetEntity">
        SELECT *
        FROM `evaporativeset` WHERE `id` = #{id}
    </select>

    <select id="findByRdcId" parameterType="map" resultType="EvaporativeSetEntity">
        SELECT *
        FROM `evaporativeset` WHERE `rdcId` = #{rdcId}
    </select>

</mapper>