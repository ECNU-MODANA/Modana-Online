<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.bgzigbee.manage.dao.ApInfoMapper">

	<select id="findInfoByApid" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.ApInfoEntity">
		SELECT *
		FROM `apInfo` WHERE `apid` = #{apid}
	</select>

</mapper>