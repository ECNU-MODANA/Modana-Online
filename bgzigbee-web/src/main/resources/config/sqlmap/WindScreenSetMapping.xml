<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smartcold.bgzigbee.manage.dao.WindScreenSetMapping">
    <insert id="insert">
        INSERT INTO `windscreenset` (`coldStorageId`,`name`)
        VALUES (#{coldStorageId}, #{name})
    </insert>
    <select id="findById" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.WindScreenSetEntity">
        SELECT *
        FROM `windscreenset` WHERE `id` = #{id}
    </select>

    <select id="findByStorageId" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.WindScreenSetEntity">
        SELECT *
        FROM `windscreenset` WHERE `coldstorageid` = #{storageid}
    </select>
</mapper>