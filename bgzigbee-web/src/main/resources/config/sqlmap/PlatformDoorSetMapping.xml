<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smartcold.bgzigbee.manage.dao.PlatformDoorSetMapping">
    <insert id="insert">
        INSERT INTO `platformdoorset` (`rdcid`, `name`, `length`, `width`)
        VALUES (#{rdcid}, #{name}, #{length}, #{width})
    </insert>
    <select id="findById" parameterType="map" resultType="PlatformDoorSetEntity">
        SELECT *
        FROM `platformdoorset` WHERE `id` = #{id}
    </select>

    <select id="findByRdcId" parameterType="map" resultType="PlatformDoorSetEntity">
        SELECT *
        FROM `platformdoorset` WHERE `rdcId` = #{rdcId}
    </select>
</mapper>