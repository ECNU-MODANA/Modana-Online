<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smartcold.bgzigbee.manage.dao.EvaporativeBlowerSetMapping">
    <insert id="insert">
        INSERT INTO `evaporativeblowerset` (`groupid`, `name`)
        VALUES (#{groupid}, #{name})
    </insert>
    <select id="findById" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.EvaporativeBlowerSetEntity">
        SELECT *
        FROM `evaporativeblowerset` WHERE `id` = #{id}
    </select>

    <select id="findByGroupid" parameterType="map" resultType="com.smartcold.bgzigbee.manage.entity.EvaporativeBlowerSetEntity">
        SELECT *
        FROM `evaporativeblowerset` WHERE `groupid` = #{groupid}
    </select>
</mapper>