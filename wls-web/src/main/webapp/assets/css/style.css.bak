    border-left:none;
    border-right:8px solid #f5f5f5;
}
.message-body .left-p{
    margin-left:8px;
    background:#f5f5f5;
    padding:10px 10px;
}
.message-body .right-span{
    position:absolute;
    top:50%;
    margin-top:-13px;
    right:0px;
    display:block;
    width:0;
    height:0;
    overflow:hidden;
    line-height:0;
    font-size:0;
    border-bottom:8px solid #FFF;
    border-top:8px solid #FFF;
    border-right:none;
    border-left:8px solid #f5f5f5;
}
.message-body .right-p{
    margin-right:8px;
    background:#f5f5f5;
    padding:10px 10px;
}
