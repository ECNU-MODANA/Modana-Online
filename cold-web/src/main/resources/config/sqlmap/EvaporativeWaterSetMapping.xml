<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.manage.cold.dao.olddb.EvaporativeWaterSetMapping">

    <select id="findById" parameterType="map" resultType="EvaporativeWaterSetEntity">
		SELECT *
		FROM `evaporativewaterset` WHERE `id` = #{id}
	</select>
	
	<select id="findByEvaporativeId" parameterType="map" resultType="EvaporativeWaterSetEntity">
		SELECT *
		FROM `evaporativewaterset` WHERE `evaporativeId` = #{evaporativeId}
	</select>

</mapper>