<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smartcold.manage.cold.dao.newdb.StorageKeysMapper">
	<select id="findAll" resultType="StorageKeysEntity">
		select * from storagekeys
	</select>

	<insert id="save" parameterType="StorageKeysEntity">
		INSERT INTO storagekeys
			(`type`,`key`, `desc`, `unit`) VALUES
			(#{type},#{key}, #{desc}, #{unit})
	</insert>
</mapper>