<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smartcold.manage.cold.dao.newdb.NewStorageKeysMapper">
	<select id="findAll" resultType="NewStorageKeysEntity">
		select * from newstoragekeys
	</select>
 	
 	<update id="saveOrUpdateByType">
 		INSERT INTO newstoragekeys (type, `key`, `desc`) VALUES(#{type}, #{key}, #{desc})
 			ON DUPLICATE KEY UPDATE `key`=#{key}, `desc`=#{desc}
 	</update>
</mapper>